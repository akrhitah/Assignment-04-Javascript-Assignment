{
  "manifest_version": 2,

  "name": "Open Public Tutorials' Website Plugin",
  "description": "This extension will open a website for public tutorials ",
  "version": "1.0",
  
  "background":{
  "scripts": ["openNew.js"]
},

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "openNew.html"
  },
  "permissions": [
   "tabs"
   ],
   "manifest_version":2
}